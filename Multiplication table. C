#include <stdio.h>

int main() {
    int num, i;

    // Input from user
    printf("Enter a number to display its multiplication table: ");
    scanf("%d", &num);

    // Generate multiplication table
    printf("Multiplication Table for %d:\n", num);
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", num, i, num * i);
    }

    return 0;
}
