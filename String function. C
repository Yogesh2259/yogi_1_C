#include <stdio.h>
#include <string.h>

int main() {
    char str1[10], str2[10], temp[10]; printf("Enter the first string: ");
    scanf("%s", str1);
    printf("Enter the second string: ");
    scanf("%s", str2);
    printf("Length of first string: %ld\n", strlen(str1));       
    strcpy( temp, str1);                                   
    printf("Copied string: %s\n", temp);
    strcat(str1, str2);                                        
    printf("Concatenated string: %s\n", str1);
    printf("Comparison result: %d\n", strcmp(temp, str2));     

    return 0;
}
